{
  "name": "@futuretense/0chain-sdk",
  "version": "0.0.4",
  "description": "js client library to interact with the 0Chain blockchain",
  "main": "lib/commonjs/index.js",
  "browser": "lib/esm/browser.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib && rm -rf temp",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:esm": "npx tsc -m es6 --outDir lib/esm",
    "build:cjs": "npx tsc -m commonjs --outDir lib/commonjs",
    "build:types": "npx tsc -p tsconfig.types.json && npx api-extractor run --local",
    "build:docs": "npx typedoc",
    "publish:docs": "npx gh-pages -m 'Updates' -d docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Johan St√©n <johan@futuretense.io>",
  "license": "MIT",
  "dependencies": {
    "@futuretense/herumi-bls": "0.0.1",
    "bip39": "^2.5.0",
    "elliptic": "^6.5.2",
    "isomorphic-fetch": "^2.2.1",
    "js-sha3": "^0.8.0",
    "secure-random": "^1.1.2",
    "with-query": "^1.3.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.7.12",
    "@types/node": "^12.12.34",
    "gh-pages": "^2.2.0",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  }
}
